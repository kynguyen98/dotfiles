alias update="sudo pacman -Syyu"
alias install="sudo pacman -S"
alias search="sudo pacman -Ss"
alias clean="sudo pacman -Sc"
alias st="st-flash"
alias remove="sudo pacman -Rnsc"
alias v="nvim"
